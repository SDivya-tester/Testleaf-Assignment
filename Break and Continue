package week1.day2;

public class BreakandContinue {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;
		for(i=1;i<10;i++) {
			if(i==4) {
				break;
				}
			System.out.println("Break :"+i);
		}
		for(i=1;i<10;i++) {
			if(i==4) {
				continue;
				}
			System.out.println("Continue :"+i);
		}
	}

}
